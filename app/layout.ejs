<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title><%- title %></title>
    <link
      href="https://fonts.googleapis.com/icon?family=Material+Icons"
      rel="stylesheet"
    />

    <link rel="stylesheet" href="./assets/style/all.css" />
  </head>
  <body>
    <div class="container">
      <nav class="navbar navbar-expand-md px-0">
        <button
          class="navbar-toggler"
          type="button"
          data-toggle="collapse"
          data-target="#navbarNav"
          aria-controls="navbarNav"
          aria-expanded="false"
          aria-label="Toggle navigation"
        >
          <i class="material-icons">dehaze</i>
        </button>
        <h1>
          <a class="navbar-brand" href="#">
            <img src="/assets/images/logo-aloha.svg" />
          </a>
        </h1>

        <div class="collapse navbar-collapse ml-auto" id="navbarNav">
          <ul class="navbar-nav">
            <li class="nav-item">
              <div class="dropdown">
                <a
                  class="btn dropdown-toggle"
                  id="langDropdown"
                  data-toggle="dropdown"
                  aria-haspopup="true"
                  aria-expanded="false"
                >
                  <i class="material-icons">language</i>
                </a>
                <div class="dropdown-menu" aria-labelledby="langDropdown">
                  <a class="dropdown-item" href="#">English</a>
                  <a class="dropdown-item" href="#">Francais</a>
                  <a class="dropdown-item" href="#">Deutsch</a>
                  <a class="dropdown-item" href="#">Simplified Chinese </a>
                  <a class="dropdown-item" href="#">Traditional Chinese </a>
                </div>
              </div>
            </li>
            <li class="nav-item">
              <a class="nav-link" href="#">Sign up</a>
            </li>
          </ul>
        </div>
        <a class="nav-item" href="#">Login</a>
      </nav>
    </div>
    <%- contents %>
    <div class="container">
      <div class="footer align-items-center justify-content-md-between">
        <span class="mr-0 mr-md-8"
          >Â© 2020 ALOHA, Inc. All rights reserved.</span
        >
        <ul class="list-unstyled d-flex mb-md-0">
          <li>FAQs</li>
          <li>Terms of use</li>
          <li>Privacy policy</li>
        </ul>
        <div class="dropdown ml-auto mr-4">
          <a
            class="btn dropdown-toggle"
            id="langDropdown"
            data-toggle="dropdown"
            aria-haspopup="true"
            aria-expanded="false"
          >
            <i class="material-icons">language</i>
            <span>English</span>
          </a>
          <div
            class="dropdown-menu dropdown-menu-right"
            aria-labelledby="langDropdown"
          >
            <a class="dropdown-item" href="#">English</a>
            <a class="dropdown-item" href="#">Francais</a>
            <a class="dropdown-item" href="#">Deutsch</a>
            <a class="dropdown-item" href="#">Simplified Chinese </a>
            <a class="dropdown-item" href="#">Traditional Chinese </a>
          </div>
        </div>
        <div class="dropdown">
          <a
            class="btn dropdown-toggle"
            id="currencyDropdown"
            data-toggle="dropdown"
            aria-haspopup="true"
            aria-expanded="false"
          >
            <i class="material-icons">attach_money</i>
            <span class="currencyName">AUD</span>
          </a>
          <div
            class="dropdown-menu dropdown-menu-right"
            aria-labelledby="currencyDropdown"
          >
            <a class="dropdown-item" href="#">
              <i class="material-icons">attach_money</i>
              <span class="currencyName">USD</span>
            </a>
            <a class="dropdown-item" href="#">
              <i class="material-icons">euro_symbol</i>
              <span class="currencyName">EUR</span>
            </a>
            <a class="dropdown-item" href="#">
              <i class="material-icons">attach_money</i>
              <span class="currencyName">CAD</span>
            </a>
          </div>
        </div>
      </div>
    </div>

    <!-- Bootstrap 4 JS  -->
    <script
      src="https://code.jquery.com/jquery-3.3.1.slim.min.js"
      integrity="sha384-q8i/X+965DzO0rT7abK41JStQIAqVgRVzpbzo5smXKp4YfRvH+8abtTE1Pi6jizo"
      crossorigin="anonymous"
    ></script>
    <script
      src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.14.7/umd/popper.min.js"
      integrity="sha384-UO2eT0CpHqdSJQ6hJty5KVphtPhzWj9WO1clHTMGa3JDZwrnQq4sF86dIHNDz0W1"
      crossorigin="anonymous"
    ></script>
    <script
      src="https://stackpath.bootstrapcdn.com/bootstrap/4.3.1/js/bootstrap.min.js"
      integrity="sha384-JjSmVgyd0p3pXB1rRibZUAYoIIy6OrQ6VrjIEaFf/nJGzIxFDsf4x0xIM+B07jRM"
      crossorigin="anonymous"
    ></script>
    <!-- Swiper JS -->
    <script src="https://unpkg.com/swiper/js/swiper.js"></script>
    <script>
      $(document).ready(function () {
        //initialize swiper when document ready
        var heroSwiper = new Swiper('.hero.swiper-container', {
          // Optional parameters
          slidesPerView: 1,
          spaceBetween: 0,
          loop: true,
          grabCursor: true,
          autoplay: {
            delay: 5000,
            disableOnInteraction: false,
          },
          navigation: {
            nextEl: '.swiper-button-next',
            prevEl: '.swiper-button-prev',
          },
        });

        var topChoicesSwiper = new Swiper('.topChoices.swiper-container', {
          // Optional parameters
          slidesPerView: 1.25,
          spaceBetween: 30,
          grabCursor: true,

          // navigation: {
          //   nextEl: '.swiper-button-next',
          //   prevEl: '.swiper-button-prev',
          // },

          breakpoints: {
            768: {
              slidesPerView: 2.5,
              spaceBetween: 30,
            },

            1200: {
              slidesPerView: 4,
              spaceBetween: 30,
            },
          },
        });

        var getInspire = new Swiper('.getInspire.swiper-container', {
          // Optional parameters
          slidesPerView: 1.25,
          spaceBetween: 30,
          grabCursor: true,

          // navigation: {
          //   nextEl: '.swiper-button-next',
          //   prevEl: '.swiper-button-prev',
          // },

          breakpoints: {
            768: {
              slidesPerView: 3,
              spaceBetween: 30,
            },
          },
        });
      });
    </script>
  </body>
</html>
